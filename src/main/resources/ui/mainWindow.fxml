                                    </valueFactory>

                                </Spinner>
                                <Spinner fx:id="spPropCruce" editable="true" GridPane.columnIndex="1"
                                         GridPane.rowIndex="5">
                                    <valueFactory>
                                        <SpinnerValueFactory.DoubleSpinnerValueFactory amountToStepBy="0.05"
