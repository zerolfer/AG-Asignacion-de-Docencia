        <Label text="Fichero de asignaturas"/>
        <TextField fx:id="tfAsignaturasFile" GridPane.columnIndex="1"/>
        <Button fx:id="btnExlorarAsignaturas" mnemonicParsing="false" onAction="#SeleccionFicheroAsignaturas"
